        "163746679909a0cbe2-2": {
          "type": "testimonial",
          "settings": {
            "icon": "quote",
            "testimonial": "<p>The price was right. The shipping was quick. The packaging was perfect. How is customer service this good in the 21st century? Keep doing simple & good work.<\/p>",
            "image": "shopify:\/\/shop_images\/UGC-2.png",
            "author": "Michael V.",
